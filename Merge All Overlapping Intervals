vector<vector<int>> mergeOverlappingIntervals(vector<vector<int>> &arr){
vector<vector<int>> ans;
int first=arr[0][0];
int second=arr[0][1];
for(int i=1;i<arr.size();i++)
{
if(second<arr[i][0])
{
ans.push_back({first,second});
first=arr[i][0];
second=arr[i][1];
}
else if(second<arr[i][1])
second=arr[i][1];
}
ans.push_back({first,second});
return ans;
}
