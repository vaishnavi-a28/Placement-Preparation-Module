
int length(Node* head){
    int len=0;
    while(head!=NULL){
        len++; 
        head = head->next;
    }
    return len;
}
Node *findMiddle(Node *head) {
    int len = length(head);
    len/=2;
    Node* temp = head;
    for(int i=0;i<len;i++){
        temp=temp->next;
    }
    return temp;
}
